function findNthLargest(arr, n) {
    // Ensure n is within the bounds of the array
    if (n < 1 || n > arr.length) {
        return "Invalid input: n out of range";
    }

    // Return the nth largest element
    return arr[arr.length - n];
}

// Example usage:
const arr = [1, 3, 5, 7, 9];
const n = 2;
console.log(findNthLargest(arr, n)); // Output: 7
