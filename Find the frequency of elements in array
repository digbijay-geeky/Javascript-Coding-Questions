function find(arr){
    var hmap=new Map();
    for(var i=0;i<array.length;i++){
        if(hmap.has(arr[i])){
            hmap.set(arr[i],hmap.get(arr[i])+1)
        }
        else{
            hmap.set(arr[i],1)
        }
    }
    return hmap;
}
var array=[1,1,2,33,33,33,2,8,3,8]
console.log(find(array))
