function rotateArray(arr,pos){
    var arr1=[];
    var arr2=[];
   for(var i=0;i<arr.length-2;i++){
       arr1.push(arr[i])
   }
   
   for(var i=pos+1;i<arr.length;i++){
       arr2.push(arr[i])
   }

   var arr3=[...arr2,...arr1]
   return arr3
}
const arr = [1, 2, 3, 4, 5];
const pos = 2;
const result = rotateArray(arr, pos);
console.log(result)
